import React from "react";

interface ImgProps {
    src: string;
    alt?: string;
    className?: string;
}

const Img: React.FC<ImgProps> = ({ src, alt, className }: ImgProps) => {
    return (
        <img
            className={className || ""}
            alt={alt || ""}
            src={src}
        />
    );
};

export default Img;
